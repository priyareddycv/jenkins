pipeline {
   
   agent any 
    
    environment{
        GIT_URL = 'https://github.com/priyareddycv/jenkins.git'
        }
    stages{
        stage ( 'TEST1' ) {
		        when {
                        branch 'master' 
                        }
		           steps { 
				sh '''
			             echo "this is executed from TEST1 stage and branch is master"
			           '''
				  }
				}
        
		    stage ( 'TEST2' ) {
		
		        when {
			        branch 'master'
				    }
					    steps {
				
				            sh  '''
					          echo " this is executed from TEST2 stage and branch is master "
				          }
					 }
			}
                       }
